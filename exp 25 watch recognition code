import cv2
watch = cv2.imread("watch.jpg", 0)
test = cv2.imread("test_image.jpg", 0)
if watch is None or test is None:
    print("Image not found")
    exit()
orb = cv2.ORB_create()
kp1, des1 = orb.detectAndCompute(watch, None)
kp2, des2 = orb.detectAndCompute(test, None)
bf = cv2.BFMatcher(cv2.NORM_HAMMING, crossCheck=True)
matches = bf.match(des1, des2)
matches = sorted(matches, key=lambda x: x.distance)
result = cv2.drawMatches(watch, kp1, test, kp2, matches[:20], None, flags=2)
cv2.imshow("Watch Recognition", result)
cv2.waitKey(0)
cv2.destroyAllWindows()
