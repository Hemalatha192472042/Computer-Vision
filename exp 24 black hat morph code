import cv2
import numpy as np
img = cv2.imread("car.jpg")
if img is None:
    print("Image not found. Check the file name and path.")
    exit()
gray = cv2.cvtColor(img, cv2.COLOR_BGR2GRAY)
kernel = np.ones((5, 5), np.uint8)
blackhat_image = cv2.morphologyEx(gray, cv2.MORPH_BLACKHAT, kernel)
cv2.imshow("Original Grayscale Image", gray)
cv2.imshow("Black-Hat Operation Result", blackhat_image)
cv2.waitKey(0)
cv2.destroyAllWindows()
